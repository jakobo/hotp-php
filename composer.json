{
    "name": "jakobo/hotp-php",
    "description": "HOTP simplifies One Time Password systems for PHP Authentication",
    "license": "BSD-3-Clause",
    "authors" : [
        {
            "name": "Jakob Heuser",
            "email": "jakob@felocity.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "jakobo\\HOTP\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.13",
